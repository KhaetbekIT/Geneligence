{
    "version": 2,
    "builds": [
      {
        "src": "/backend/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["/backenddb.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "/backend/index.js"
      }
    ]
  }